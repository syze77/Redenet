{
  "compilerOptions": {
    "outDir": "./dist/out-tsc",  
    "rootDir": "./src/",  
    "strict": true,
    "esModuleInterop": true,  
    "moduleResolution": "node",  
    "target": "ES2022",  
    "module": "ES2022",
    "sourceMap": true,
    "declaration": false,
    "skipLibCheck": true,
    "experimentalDecorators": true
  },
  "include": [
    "src/**/*.ts", 
    "bot.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
